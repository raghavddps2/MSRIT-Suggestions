  body#page-top
    // / Nav Star /
    nav#mainNav.navbar.navbar-b.navbar-trans.navbar-expand-md.fixed-top
      .container
        a.navbar-brand.js-scroll(href='#page-top') MSRIT Suggest
        button.navbar-toggler.collapsed(type='button', data-toggle='collapse', data-target='#navbarDefault', aria-controls='navbarDefault', aria-expanded='false', aria-label='Toggle navigation')
          span
          span
          span
        #navbarDefault.navbar-collapse.collapse.justify-content-end
          ul.navbar-nav
            li.nav-item
              a.nav-link.js-scroll.active(href='/') Home
            li.nav-item
              a.nav-link.js-scroll(href='/view-suggesstions') View Sugestions
            li.nav-item
              a.nav-link.js-scroll(href='/add-suggesstion') Add suggestions
    // / Nav End /
    .container-contact100
      .wrap-contact100
        form.contact100-form.validate-form(action='/sugAdd', method='POST')
          span.contact100-form-title
            | Suggestion!
          .wrap-input100.validate-input(data-validate='Title is required')
            span.label-input100 Title
            input.input100(type='text', name='title', placeholder='Enter title')
            span.focus-input100
          .wrap-input100.validate-input(data-validate='Tags are required')
            span.label-input100 Tags
            input.input100(type='text', name='tag', placeholder='Enter one or two tags')
            span.focus-input100
          .wrap-input100.input100-select
            span.label-input100 Department
            div
              select.selection-2(name='service')
                option ISE
                option CSE
                option ECE
                option EIE
                option TCE
                option Other
            span.focus-input100
          .wrap-input100.validate-input(data-validate='Message is required')
            span.label-input100 Suggestion
            textarea.input100(name='message', placeholder='Please be polite while writing the suggestion')
            span.focus-input100
          .container-contact100-form-btn
            .wrap-contact100-form-btn
              .contact100-form-bgbtn
              button.contact100-form-btn
                span
                  | Submit
                  i.fa.fa-long-arrow-right.m-l-7(aria-hidden='true')
    #dropDownSelect1
